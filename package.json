  "license": "MIT",
  "dependencies": {
    "debug": "~2.2.0",
    "eslint": "~3.19.0",
    "http-parser-js": "~0.4.4",
    "moment": "~2.13.0",
    "on-finished": "~2.3.0",
    "verror": "~1.8.1",
    "xtend": "~4.0.1"
  },
  "devDependencies": {
    "async": "~2.0.1",
    "chai": "~3.5.0",
    "coveralls": "~2.13.0",
    "expeditious": "~0.1.0",
    "expeditious-engine-memory": "~0.1.0",
    "express": "~4.14.0",
    "linelint": "~1.0.0",
    "mocha": "~2.5.3",
    "mocha-lcov-reporter": "~1.3.0",
    "nyc": "~6.6.1",
    "proxyquire": "~1.7.9",
    "request": "~2.72.0",
    "sinon": "~1.17.4",
    "supertest": "~1.2.0"
  }
